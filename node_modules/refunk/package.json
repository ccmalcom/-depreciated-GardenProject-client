{
  "name": "refunk",
  "version": "3.0.1",
  "description": "Simple functional setState for React",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "babel src -d dist",
    "size": "bundlesize",
    "cover": "nyc report --reporter=html",
    "test": "nyc ava"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bundlesize": "^0.17.0",
    "nyc": "^11.2.1",
    "react": "^16.3.0",
    "react-test-renderer": "^16.3.0"
  },
  "dependencies": {
    "prop-types": "^15.5.10"
  },
  "keywords": [
    "react",
    "state",
    "functional",
    "context",
    "render-props",
    "hoc"
  ],
  "author": "Brent Jackson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jxnblk/refunk.git"
  },
  "bugs": {
    "url": "https://github.com/jxnblk/refunk/issues"
  },
  "homepage": "https://github.com/jxnblk/refunk#readme",
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "bundlesize": [
    {
      "path": "src/index.js",
      "maxSize": "0.5 kB"
    },
    {
      "path": "dist/index.js",
      "maxSize": "1.5 kB"
    }
  ]
}
